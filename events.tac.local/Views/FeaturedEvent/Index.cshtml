@model events.tac.local.Models.FeaturedEvent

<div class="col-sm-4">
    <div class="thumbnail well @Model.CssClass">
        <a href="@Model.UrlLink">
            @Model.EventImage
        </a>
        <div class="caption">
            <h3 class="teaser-heading">
                @Model.EventHeading
            </h3>

            <p>
                @Model.EventIntro
            </p>

            <a href="@Model.UrlLink" class="btn btn-default">
            Read more
            </a>
        </div>
    </div>
</div>
