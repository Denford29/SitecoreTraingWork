@model List<SATC.SC.Framework.Navigation.Models.NavigationItemModel>

@if (Model.Count > 0)
{
    <!-- [Breadcrumb Navigation] -->
    <ol class="breadcrumb">
        @foreach (var navItem in Model)
        {
            <li class="@(navItem.Active ?"active": string.Empty )">
                <a href="@navItem.Url" title="@navItem.Title">
                    @navItem.Title
                </a>
            </li>
        }
    </ol>
    <!-- [/Breadcrumb Navigation] --> 
}
